@model IEnumerable<_18_02_2021_University_manager.Models.GroupViewModels>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("SentRequest", "SentRequest", new { name = item.Name }) |
        </td>
    </tr>
}

</table>
